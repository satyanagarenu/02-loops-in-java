import java.util.Scanner;

public class Main {
    public static void main(String[] args){
        Scanner obj=new Scanner(System.in);
        int a=obj.nextInt();
        int b=0;
        int c=a;
        while(a>0){
            b=b*10+a%10;
            a=a/10;

        }
        System.out.print(b);
        if(c==b)
            System.out.print(" palindrome");

    }
}
output:
12321
12321 palindrome
